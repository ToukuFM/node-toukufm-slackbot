<html lang="en" ng-app="toukuApp">

<head>
    <!-- Meta -->
    <title>Roundup</title>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />

    <!-- Styles -->
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="/css/ng-mfb.min.css">
    <link rel="stylesheet" href="/css/app.css">
</head>


<body layout="column" ng-controller="appCtrl">
    <!-- <% include ./partials/preloader %> -->

    <md-toolbar id="top-bar" layout="row">
        <% include ./partials/toolbar %>
    </md-toolbar>

    <div layout="row" flex>

        <md-sidenav layout="column" class="md-sidenav-left md-whiteframe-z2" md-component-id="sidenav" md-is-locked-open="$mdMedia('gt-md')">
            <% include ./partials/sidenav %>
        </md-sidenav>

        <md-content layout="column" id="main-content" flex class="md-padding">
            <% include ./partials/main %>
        </md-content>

    </div>

    <% include ./partials/fab %>

    <!-- Scripts -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js"></script>
    <script src="/js/ng-mfb.js"></script>
    <script src="/js/app.js"></script>
</body>

</html>